<%- include('_head.ejs', {title: 'Table'}) %>
<table style="width:100%">
  <tr>
  <% columns.forEach((c)=>{ %>
    <th><%= c %></th>
  <% }) %>
    <th>Modify</th>
  </tr>

  <% rows.forEach((r)=>{ %>
  <tr>
    <% columns.forEach((c)=>{ %>
    <td><%= r[c] %></td>
    <% }) %>
    <td><input type="submit" name="<%r.ID%>>" value="Modify"></td>
  </tr>
  <% }) %>
</table>
